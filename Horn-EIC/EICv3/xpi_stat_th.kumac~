set *
 close 0

opt nbox

set ymgu 0.75
set xmgr 2.5
set ymgl 1.75
set xmgl 3.0
set *siz 0.45
set ysiz 20
set xsiz 18
set xval 0.7
set xlab 2.3

opt utit
title ' ' u

opt nsta
set *fon -130
  
 hi/fil 21 rapgap_q23.75.his 0 
 hi/fil 22 rapgap_q215.his 0
 hi/fil 23 rapgap_q260.his 0
 hi/fil 24 rapgap_q2240.his 0
 
 for/fil 88 x_vs_counts_stat.eps
 meta 88 -113
 
 zone 2 2
 
 opt linx
 opt logy
 set hwid 3
 set hcol 4
 h/pl //lun21/106
* itx 0.4 3E5 'Q2=2.5'
 itx 0.4 3E5 'Q2=3.75'
 atitle 'x?[p]!' 'Counts' ! 220   
 
 mess $hinfo(106,'entries')
 mess $hinfo(106,'sum')
 
 
 h/pl //lun22/106
 itx 0.4 1E5 'Q2=15' 
 atitle 'x?[p]!' '' ! 220   
 h/pl //lun23/106
 itx 0.5 1E5 'Q2=60' 
 atitle 'x?[p]!' 'Counts' ! 220   
 h/pl //lun24/106
  itx 0.1 0.7E5 'Q2=240'
 atitle 'x?[p]!' '' ! 220  

 close 88
